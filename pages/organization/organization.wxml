        <view class="cardTop organization">
            <view class="iconfont icon-zuzhibumen cardIcon"></view>
        </view>
        <view class="cardText">
            校级组织
        </view>
    </navigator>
</view>

<view class="mark">
    社团
</view>

<view class="cardContainer">
    <navigator class="card" url="../informationList/informationList?type=学术科技类">
        <view class="cardTop academic">
            <view class="iconfont icon-kejichuangxin cardIcon"></view>
        </view>
        <view class="cardText">
            学术科技类
        </view>
    </navigator>
    <navigator class="card" url="../informationList/informationList?type=公益实践类">
        <view class="cardTop publicWelfare">
